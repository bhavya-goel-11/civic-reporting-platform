{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
  "skipLibCheck": true,
    "strict": true,
    "jsx": "react-native",
    "esModuleInterop": true,
    "baseUrl": "./",
    "typeRoots": [
      "./types",
      "../node_modules/@types"
    ],
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
